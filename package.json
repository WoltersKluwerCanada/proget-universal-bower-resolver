{
  "name": "proget-universal-bower-resolver",
  "version": "0.4.0",
  "description": "The ProGet Universal Bower resolver",
  "main": "lib/index.js",
  "keywords": [
    "bower-resolver",
    "proget"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/WoltersKluwerCanada/proget-universal-bower-resolver"
  },
  "bugs": {
    "url": "https://github.com/WoltersKluwerCanada/proget-universal-bower-resolver/issues"
  },
  "author": "Alexandre Beaudoin <alexandre.beaudoin@wolterskluwer.com>",
  "license": "MIT",
  "dependencies": {
    "adm-zip": "^0.4.7",
    "request": "^2.72.0",
    "retry": "^0.10.0",
    "semver": "^5.2.0",
    "tmp": "^0.0.31"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "coveralls": "^2.11.14",
    "eslint": "^3.6.0",
    "istanbul": "^0.4.3",
    "jsdoc": "^3.4.0",
    "minami": "^1.1.1",
    "mocha": "^3.0.0",
    "rimraf": "^2.5.2",
    "tslint": "^4.1.1",
    "typescript": "^2.1.4",
    "typings": "^2.0.0"
  },
  "scripts": {
    "buildDeploy": "typings install && rimraf lib/* && tsc --sourceMap false",
    "coverage": "istanbul cover node_modules/mocha/bin/_mocha -R ./test/*.test.js",
    "coveralls": "istanbul cover node_modules/mocha/bin/_mocha -R ./test/*.test.js && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "jsDoc": "jsdoc --configure .jsdoc.json --verbose",
    "lint": "tslint --project tsconfig.json",
    "lintTest": "eslint test/",
    "lintAllFix": "tslint --project tsconfig.json --fix & eslint test/ --fix",
    "prepublish": "npm run buildDeploy",
    "test": "npm run buildDeploy && npm run lint && npm run lintTest && npm run unit && npm run coverage",
    "unit": "mocha -R spec test/*.test.js"
  },
  "engines": {
    "node": ">=4"
  }
}
