{
  "name": "proget-universal-bower-resolver",
  "version": "0.5.4",
  "description": "The ProGet Universal Bower resolver",
  "main": "bin/main.js",
  "keywords": [
    "bower-resolver",
    "proget"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/WoltersKluwerCanada/proget-universal-bower-resolver"
  },
  "bugs": {
    "url": "https://github.com/WoltersKluwerCanada/proget-universal-bower-resolver/issues"
  },
  "author": "Alexandre Beaudoin <alexandre.beaudoin@wolterskluwer.com>",
  "license": "MIT",
  "dependencies": {
    "extract-zip": "^1.6.7",
    "request": "^2.88.0",
    "retry": "^0.12.0",
    "rimraf": "^2.6.3",
    "semver": "^5.6.0",
    "tmp": "^0.0.33"
  },
  "devDependencies": {
    "@types/extract-zip": "^1.6.2",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.12.18",
    "@types/request": "^2.48.1",
    "@types/rimraf": "2.0.2",
    "@types/semver": "^5.5.0",
    "@types/tmp": "0.0.33",
    "@types/retry": "0.12.0",
    "chai": "^4.2.0",
    "coveralls": "^3.0.2",
    "istanbul": "^1.1.0-alpha.1",
    "mocha": "^5.2.0",
    "nyc": "^13.1.0",
    "ts-node": "^6.2.0",
    "tslint": "^5.12.0",
    "typescript": "^2.9.2"
  },
  "scripts": {
    "buildDeploy": "rimraf lib/* && tsc",
    "coverage": "nyc npm run test:unit",
    "coveralls": "nyc report --reporter=text-lcov npm run test:unit | coveralls",
    "lint": "tslint --project tsconfig.json",
    "lint:Fix": "tslint --project tsconfig.json --fix",
    "prepublishOnly": "npm run buildDeploy",
    "test": "npm run buildDeploy && npm run lint && npm run coverage",
    "test:unit": "mocha --require ts-node/register --bail test/*.spec.ts"
  },
  "engines": {
    "node": ">=8"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "node_modules/",
      "dist/"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "sourceMap": true,
    "instrument": true
  }
}
